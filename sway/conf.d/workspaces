# Switch to workspace
bindsym $mod+1 workspace $ws_term
bindsym $mod+2 workspace $ws_web
bindsym $mod+3 workspace $ws_work
bindsym $mod+4 workspace $ws_docs
bindsym $mod+5 workspace $ws_media
bindsym $mod+6 workspace $ws_music
bindsym $mod+7 workspace $ws_code
bindsym $mod+8 workspace number 8
bindsym $mod+9 workspace number 9
bindsym $mod+Tab workspace next
bindsym $mod+Shift+Tab workspace prev
bindsym $mod+Backslash workspace prev
bindsym Ctrl+Shift+Tab workspace prev
bindsym Ctrl+Backslash workspace prev
# Move focused container to workspace
bindsym $mod+Shift+1 move container to workspace $ws_term, workspace $ws_term
bindsym $mod+Shift+2 move container to workspace $ws_web, workspace $ws_web
bindsym $mod+Shift+3 move container to workspace $ws_work, workspace $ws_work
bindsym $mod+Shift+4 move container to workspace $ws_docs, workspace $ws_docs
bindsym $mod+Shift+5 move container to workspace $ws_media, workspace $ws_media
bindsym $mod+Shift+6 move container to workspace $ws_music, workspace $ws_music
bindsym $mod+Shift+7 move container to workspace $ws_code, workspace $ws_code
bindsym $mod+Shift+8 move container to workspace number 8
bindsym $mod+Shift+9 move container to workspace number 9
# Note: workspaces can have any name you want, not just numbers.
# We just use 1-10 as the default.
